{
  "name": "Custom Payment Provider",
  "id": "PaysecureID",
  "version": "1.0.0",
  "supportedLocales": ["en"],
  "capabilities": {
    "payments": {
      "supported": true,
      "currencies": ["USD"],
      "paymentMethods": ["card", "upi", "netbanking", "wallet"],
      "recurring": false
    }
  },
  "endpoints": {
    "payment": {
      "create": "/api/redirect",
      "capture": "/api/payments/capture",
      "refund": "/api/payments/refund",
      "cancel": "/api/payments/cancel"
    },
    "callback": "/api/callback"
  }
}